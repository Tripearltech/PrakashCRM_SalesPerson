
@{
    ViewBag.Title = "SalesQuoteList";
    Layout = "~/Views/Shared/_SalespersonMasterLayout.cshtml";
}

<style>
    .SQverticalScroll {
        padding: 15px;
        overflow-y: auto;
        height: 500px;
    }
    .QtyCountCls, .ScheduleOrderCls,
    .EditCls:hover {
        cursor: pointer;
    }
</style>
<link href="~/Content/appcss/paginationFilterCss.css" rel="stylesheet" />
<div class="page-content">
    <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
        <div class="ps-3">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb mb-0 p-0">
                    <li class="breadcrumb-item">
                        <a href="/SPDashboard/Index"><i class="bx bx-tachometer"></i>&nbsp;Dashboard</a>
                    </li>
                    <li class="breadcrumb-item active" aria-current="page">Sales Quote List</li>
                </ol>
            </nav>
        </div>
    </div>
    <hr />
    <div class="card">
        <div class="card-body">

            <div class="row">
                <div>
                    <div class="row g-3">
                        <div class="col-md-2">
                            <input id="hfFirstRowDropShipmentStatus" value="" style="display:none" />
                            <input id="hfSQNo" value="" style="display:none" />
                            <input id="hfProdNo" value="" style="display:none" />
                            <input id="hfSchProdNo" value="" style="display:none" />
                            <input id="hfLocCode" value="" style="display:none" />
                            <input id="hfTRSQProdNo" value="" style="display:none" />
                            <input id="hdnSPEmail" value="@Session["loggedInUserEmail"]" style="display:none" />
                            <input id="getServiceApiUrl" value="@System.Configuration.ConfigurationManager.AppSettings["ServiceApiUrl"].ToString()" style="display:none" />
                            <input id="hfShortcloseType" style="display:none" />
                            <button id="btnCreateSQ" class="btn btn-primary  mb-3 mb-lg-0" type="button"><i class='bx bxs-plus-square'></i>&nbsp;Create Sales Quote</button>
                        </div>

                        <div class="col-md-10">
                            <div class="row row-cols-auto g-3" style="float:right">
                                <div class="col">
                                    <select id="ddlStatus" class="form-select">
                                        <option value="-1">All</option>
                                        <option value="Pending" selected>Pending</option>
                                        <option value="Partial">Partial</option>
                                        <option value="Completed">Completed</option>
                                    </select>
                                </div>
                                <div class="col">
                                    <label class="col-form-label">Filter:</label>
                                </div>
                                <div class="col">
                                    <select id="ddlField" class="form-select">
                                        <option value="-1">---Select---</option>
                                        <option value="No">Quote No</option>
                                        <option value="Sell_to_Customer_Name">Customer Name</option>
                                        <option value="Order_Date">Quote Date</option>
                                        <option value="Amount">Amount</option>
                                        <option value="TPTPL_Short_Close">Shortclose</option>
                                    </select>
                                </div>
                                <div class="col">
                                    <input id="txtFromDate" type="date" class="form-control" style="display:none">
                                    <select id="ddlOperator" class="form-select">
                                        <option value="-1">---Select---</option>
                                        <option value="Equal">Equal</option>
                                        <option value="Not Equal">Not Equal</option>
                                        <option value="Starts With">Starts With</option>
                                        <option value="Ends With">Ends With</option>
                                        <option value="Contains" selected>Contains</option>
                                        <option value="Less Than">Less Than</option>
                                        <option value="Greater Than">Greater Than</option>
                                    </select>
                                </div>
                                <div class="col">
                                    <input id="txtToDate" type="date" class="form-control" style="display:none">
                                    <div id="dvtxtSearch" class="position-relative">
                                        <input id="txtSearch" type="text" class="form-control ps-5" placeholder="Search Product..."> <span class="position-absolute top-50 product-show translate-middle-y"><i class="bx bx-search"></i></span>
                                    </div>
                                </div>
                                <div class="col">
                                    <button id="btnSearch" class="btn btn-primary" type="button"><i class="bx bx-search"></i>Search</button>
                                    &nbsp;
                                    <button id="btnClearFilter" class="btn btn-primary" type="button">
                                        <i class="bx bx-rotate-right me-0"></i>&nbsp;Refresh
                                    </button>
                                </div>
                                <div style="display:none" class="col">
                                    <button id="btnExport" class="btn btn-primary" type="button"><i class="bx bxs-file"></i>Export</button>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </div>
    </div>

    <div class="card">
        <div class="card-body">
            <div class="table-responsive">
                <table id="dataList" class="display responsive nowrap" style="width:100%">
                    <thead>
                        <tr>
                            <th></th>
                            <th>Edit</th>
                            <th></th>
                            @*<th>SMS</th>
                    <th>Email</th>
                    <th>Print</th>*@
                            <th>Quote No</th>
                            <th>Quote Date</th>
                            <th>Customer Name</th>
                            <th>Schedule<br />Order</th>
                            <th>Total Quote<br />Qty</th>
                            <th>Schedule/<br />Ordered Qty</th>
                            <th>Invoiced<br />Qty</th>
                            <th>In Process<br />Order Qty</th>
                            <th>Payment Terms</th>
                            <th>Shortclose</th>
                            <th>Quote Approval</th>
                        </tr>
                    </thead>
                    <tbody id="tableBody">
                    </tbody>
                    <tfoot>
                        <tr>
                            <th></th>
                            <th>Edit</th>
                            <th></th>
                            @*<th>SMS</th>
        <th>Email</th>
        <th>Print</th>*@
                            <th>Quote No</th>
                            <th>Quote Date</th>
                            <th>Customer Name</th>
                            <th>Schedule<br />Order</th>
                            <th>Total Quote<br />Qty</th>
                            <th>Schedule/<br />Ordered Qty</th>
                            <th>Invoiced<br />Qty</th>
                            <th>In Process<br />Order Qty</th>
                            <th>Payment Terms</th>
                            <th>Shortclose</th>
                            <th>Quote Approval</th>
                        </tr>
                    </tfoot>
                </table>
                <input id="hdnLoggedInUserSPCode" value="@Session["loggedInUserSPCode"]" style="display:none" />
                <input id="hdnSPCodesOfReportingPersonUser" value="@Session["SPCodesOfReportingPersonUser"]" style="display:none" />
                <input id="hdnSPSQCount" style="display:none" />
                <div id="orderInfo"></div>
            </div>
            <div>
                <div class="row">
                    <div>
                        <div class="row g-3">

                            <div class="col-md-12">
                                <div class="row row-cols-auto g-3">
                                    <div class="col">
                                        <label class="col-form-label" style="font-weight:bold">Per Page Records: </label>
                                    </div>
                                    <div class="col">
                                        <select id="ddlRecPerPage" class="form-select">
                                            <option value="10">10</option>
                                            <option value="20">20</option>
                                            <option value="30">30</option>
                                        </select>
                                    </div>
                                    <div class="col">
                                        <ul class="pagination pagination-lg pager" id="myPager"></ul>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class='loader' id='divImage' style="display:none">
    <!-- MODAL CONTENT -->
    <div class='loader-content'>
        <img alt="Loading... Please wait!" src="~/Layout/assets/images/login-images/load-image-2.gif" width="100" height="100" />
    </div>
</div>
